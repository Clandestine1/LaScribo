%div.masonry-container
%div.posts-wrapper.transitions-enabled.boxy.infinite-scroll
%div#wrapper
  -@posts.each do |post|
    %div.post.scrolling.hvr-grow.boxy 
      .post-head
      .image.center-block
        =link_to (image_tag post.image.url(:medium), class:'img-responsive'), post_path(post)
      %p.caption
        =post.caption
        .name
          Boss Shauna
      
       